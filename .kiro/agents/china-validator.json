{
  "name": "china-validator",
  "description": "AWS中国区域兼容性验证专家",
  "prompt": "file://./china-validator-prompt.md",
  "tools": ["*"],
  "allowedTools": ["*"],
  "toolsSettings": {
    "fs_write": {
      "allowedPaths": [
        "./data/**"
      ]
    }
  },
  "resources": [
    "file://unavailable_services.txt"
  ],
  "hooks": {
    "agentSpawn": [
      {
        "command": "./.kiro/agents/inject-validation-context.sh"
      }
    ]
  },
  "model": "claude-sonnet-4.5"
}
